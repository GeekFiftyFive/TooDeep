{
    "name": "rocket",
    "start_look": {
        "type": "animation",
        "animation": "rocket_idle"
    },
    "behavior": {
        "on_update": {
            "script": "camera.lua",
            "variables": {
                "tolerance": 0.0,
                "startY": -540.0
            }
        },
        "move_left": {
            "script": "move.lua",
            "variables": {
                "x": -0.5,
                "y": 0.0,
                "direction": "left"
            }
        },
        "move_right": {
            "script": "move.lua",
            "variables": {
                "x": 0.5,
                "y": 0.0,
                "direction": "right"
            }
        },
        "stop_moving_left": {
            "script": "move.lua",
            "variables": {
                "x": 0.0,
                "y": 0.0,
                "direction": "left"
            }
        },
        "stop_moving_right": {
            "script": "move.lua",
            "variables": {
                "x": 0.0,
                "y": 0.0,
                "direction": "right"
            }
        },
        "thrust": {
            "script": "applyThrust.lua",
            "variables": {
                "thrust": 0.03
            }
        },
        "stop_thrust": {
            "script": "applyThrust.lua",
            "variables": {
                "thrust": -0.03
            }
        }
    },
    "physics": {
        "collision_hulls": [
            {
                "type": "box",
                "name": "main",
                "x": 0.0,
                "y": 0.0,
                "w": 149.0,
                "h": 255.0
            },
            {
                "type": "box",
                "name": "bottom",
                "intangible": true,
                "x": 0.0,
                "y": -255.0,
                "w": 149.0,
                "h": 1.0
            }
        ],
        "gravity_enabled": true
    }
}