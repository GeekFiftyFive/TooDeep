{
    "name": "rocket",
    "start_look": {
        "type": "texture",
        "asset": "rocket.png"
    },
    "behavior": {
        "move_left": {
            "script": "move.lua",
            "variables": {
                "x": -0.5,
                "y": 0.0
            }
        },
        "move_right": {
            "script": "move.lua",
            "variables": {
                "x": 0.5,
                "y": 0.0
            }
        },
        "stop_moving": {
            "script": "move.lua",
            "variables": {
                "x": 0.0,
                "y": 0.0
            }
        },
        "thrust": {
            "script": "applyThrust.lua",
            "variables": {
                "thrust": 0.03
            }
        },
        "stop_thrust": {
            "script": "applyThrust.lua",
            "variables": {
                "thrust": -0.03
            }
        }
    },
    "physics": {
        "gravity_enabled": true
    }
}